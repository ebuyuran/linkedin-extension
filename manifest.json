{
  "name": "The LinkedIn Extension",
  "description": "Leadjet Code Challenge",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.linkedin.com/in/*"],
      "js": ["user.js"],
      "css": ["user.css"]
    }
  ]
}
